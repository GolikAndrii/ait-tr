// Дано натуральное число n. Напишите программу, которая выводит равнобедренный
// прямоугольный треугольник с катетами длиной n из звёздочек.

import java.io.*;

public class Homework_15_2 {
    public static void main(String[] args) throws IOException{
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int n = Integer.parseInt(br.readLine());

        for (int rowN = 0; rowN <= n; ++rowN) {
            for (int colN = 0; colN < rowN; ++colN) {
                System.out.print('*');
            }
            System.out.println();
        }
    }
}
